{
	"ecmaFeatures": {
		"jsx": true,
		"modules": true
	},
	"rules": {
		"no-dupe-keys": 2,
		"no-dupe-args": 2,
		"comma-dangle": [2, "never"],
		"no-duplicate-case": 2,
		"no-invalid-regexp": 2,
		"valid-jsdoc": 1,
		"no-const-assign": 2,
		"no-dupe-class-members": 2,
		"no-this-before-super": 2,
		"no-shadow-restricted-names": 2,
		"no-obj-calls": 2,
		"no-control-regex": 1,
		"no-unreachable": 2,
		"use-isnan": 1,
		"no-redeclare": 2,
		"no-native-reassign": 2,
		"no-var": 2,
		"block-scoped-var": 2,
		"no-sparse-arrays": 1,
		"no-invalid-this": 2,
		"no-use-before-define": 2,
		"no-negated-in-lhs": 2,
		"no-throw-literal": 0,
		"no-empty": 1,
		"no-empty-label": 1,
		"no-empty-character-class": 1,
		"valid-typeof": 1,
		"eqeqeq": 1,
		"no-void": 2,
		"no-with": 2,
		"default-case": 1,
		"prefer-arrow-callback": 1,
		"guard-for-in": 1,
		"prefer-spread": 1,
		"arrow-body-style": 2,
		"const": 1,
		"no-undef-init": 2,
		"no-new-wrappers": 1,
		"require-yield": 1,
		"no-lone-blocks": 1,
		"no-ex-assign": 1,
		"no-multi-spaces": 0,
		"no-useless-call": 1,
		"no-useless-concat": 1,
		"yoda": [0, "never", {
			"exceptRange": false,
			"onlyEquality": false
		}],
		"no-extra-semi": 1,
		"curly": 1,
		"no-console": 1,
		"no-extra-boolean-cast": 1,
		"no-func-assign": 1,
		"no-regex-spaces": 1,
		"no-extra-parens": 1,
		"no-irregular-whitespace": 2,
		"no-unexpected-multiline": 1,
		"no-magic-numbers": 0,
		"no-multi-spaces": 0,
		"no-loop-func": 1,
		"no-multi-str": 1,
		"no-proto": 2,
		"no-new-func": 1,
		"no-return-assign": 1,
		"no-script-url": 1,
		"no-self-compare": 0,
		"object-shorthand": 0,
		"linebreak-style": 1,
		"max-len": [1, 120, 4],
		"no-mixed-spaces-and-tabs": [1, "smart-tabs"],
		"no-multiple-empty-lines": [1, {"max": 2}],
		"no-ternary": 1,
		"no-trailing-spaces": 1,
		"padded-blocks": [1, "never"],
		"quotes": [1, "double"],
		"require-jsdoc": [2, {
			"require": {
				"FunctionDeclaration": true,
				"MethodDefinition": true,
				"ClassDeclaration": true
			}
		}],
		"semi-spacing": [1, { "before": false, "after": true }],
		"space-before-blocks": 1,
		"space-before-function-paren": [1, "never"],
		"space-in-parens": [1, "never"],
		"space-return-throw-case": 1,
		"camelcase": [1, {"properties": "always"}],
		"comma-spacing": [1, {"before": false, "after": true}],
		"brace-style": [1, "stroustrup", { "allowSingleLine": true }],
		"block-spacing": [1, "always"],
		"func-style": [1, "declaration"],
		"indent": [1, "tab"]
	},
	"env": {
		"browser": true,
		"es6": true
	}
}
