{
  "name": "mumble-bot",
  "version": "0.2.1",
  "description": "A bot for mumble.",
  "main": "index.js",
  "scripts": {
    "build:web": "webpack",
    "build:server": "tsc -p tsconfig-production.json",
    "build:web:release": "webpack --config webpack.config-release.js",
    "start:web": "webpack-dev-server",
    "start:server": "TZ=UTC nodemon",
    "lint:src": "tslint --format stylish --type-check --project tsconfig.json",
    "lint:style": "stylelint --allow-empty-input src/**/*.scss",
    "test": "TZ=UTC jest --runInBand"
  },
  "jest": {
    "collectCoverage": true,
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "transform": {
      ".(ts|tsx)": "ts-jest"
    },
    "coverageReporters": ["lcov", "json", "text-summary"],
    "moduleNameMapper": {
      "^.+\\.(css|scss|sass)$": "identity-obj-proxy"
    },
    "setupTestFrameworkScriptFile": "<rootDir>/jest-setups.ts",
    "testMatch": [
      "<rootDir>/src/**/__tests__/test-*.(ts|tsx)"
    ]
  },
  "nodemonConfig": {
    "ignore": [
      "data/**/*",
      "dist/**/*",
      "server/**/*"
    ],
    "ext": "ts",
    "watch": [
      "src"
    ],
    "exec": "yarn build:server && node server serve --config-file config.yml"
  },
  "repository": "https://git.cronosx.de/prior/mumble-bot",
  "author": "Frederick <frederick@gnodtke.net>",
  "license": "GPL-3.0",
  "dependencies": {
    "async-file": "^2.0.2",
    "bind-decorator": "^1.0.11",
    "body-parser": "1.18.2",
    "canvas": "^1.6.10",
    "chokidar": "^2.0.3",
    "chunking-streams": "^0.0.8",
    "classnames": "^2.2.5",
    "clime": "^0.5.9",
    "decko": "^1.2.0",
    "express": "^4.16.3",
    "express-ws": "^3.0.0",
    "fft-js": "^0.0.11",
    "fluent-ffmpeg": "^2.1.2",
    "hyrest": "0.7.11",
    "mkdirp-promise": "^5.0.1",
    "mobx": "^4.2.0",
    "mobx-react": "^5.0.0",
    "mobx-react-devtools": "^5.0.1",
    "morgan": "^1.9.0",
    "mumble": "^0.3.13",
    "node-samplerate": "^0.2.0",
    "nodemon": "^1.17.3",
    "onecolor": "3.0.5",
    "path-to-regexp": "^2.2.0",
    "pg": "^7.4.1",
    "promise-mysql": "^3.2.1",
    "ramda": "^0.25.0",
    "react": "^16.3.2",
    "react-dom": "^16.3.2",
    "react-router-dom": "^4.2.2",
    "rng-js": "1.0.1",
    "semantic-ui-react": "^0.79.1",
    "sox-audio": "^0.3.0",
    "tsdi": "^0.20.2",
    "typeorm": "^0.2.0",
    "uuid": "^3.2.1",
    "winston": "^2.4.2",
    "winston-mysql-transport": "0.1.2",
    "yamljs": "^0.3.0"
  },
  "devDependencies": {
    "@types/body-parser": "^1.16.8",
    "@types/chokidar": "^1.7.5",
    "@types/classnames": "^2.2.3",
    "@types/jest": "^22.2.3",
    "@types/morgan": "^1.7.35",
    "@types/multer": "^1.3.6",
    "@types/node": "^9.6.6",
    "@types/ramda": "^0.25.24",
    "@types/react": "^16.3.12",
    "@types/supertest": "^2.0.4",
    "@types/uuid": "^3.4.3",
    "@types/winston": "^2.3.9",
    "@types/yamljs": "^0.2.30",
    "css-loader": "^0.28.11",
    "extract-text-webpack-plugin": "^4.0.0-beta.0",
    "git-revision-webpack-plugin": "^3.0.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "21",
    "node-sass": "^4.8.3",
    "progress-bar-webpack-plugin": "^1.11.0",
    "resolve-url-loader": "^2.3.0",
    "sass-loader": "^7.0.1",
    "stylelint": "^9.2.0",
    "supertest": "^3.0.0",
    "ts-jest": "21",
    "ts-loader": "^4.2.0",
    "tslint": "^5.9.1",
    "typescript": "^2.8.3",
    "webpack": "^4.6.0",
    "webpack-cli": "^2.0.15",
    "webpack-dev-server": "^3.1.3"
  }
}
